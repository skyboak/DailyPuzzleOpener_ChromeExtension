{
    "manifest_version": 3,
    "name": "Daily Puzzles",
    "version": "1.0",
    "description": "Open daily puzzles like Wordle and NYT Connections",
    "permissions": [
      "storage",
      "activeTab"
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_popup": "popup.html"
    }
  }
  